@model IEnumerable<MyBakery.Models.Product>
@{
    ViewBag.Title = "Product Page";
}

<div class="jumbotron">
    <h1>Bakery products</h1>
</div>

<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.ProductKey)</th>
        <th>@Html.DisplayNameFor(model => model.ProductName)</th>
        <th>@Html.DisplayNameFor(model => model.ProductPrice)</th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@Html.DisplayTextFor(model => item.ProductKey)</td>
        <td>@Html.DisplayTextFor(model => item.ProductName)</td>
        <td>@Html.DisplayTextFor(model => item.ProductPrice)</td>
    </tr>
    }

</table>